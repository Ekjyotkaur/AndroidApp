<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".UserListViewFragment">

    <ImageView
        android:id="@+id/imageView3"
        android:layout_width="150dp"
        android:layout_height="150dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:srcCompat="@tools:sample/avatars"
        app:layout_constraintTop_toTopOf="parent"
        android:layout_marginTop="80dp"/>


    <TextView
        android:id="@+id/fullNameLabel"
        android:layout_width="115dp"
        android:layout_height="wrap_content"
        android:text="Full Name"
        app:layout_constraintBottom_toTopOf="@+id/genderLabel"
        app:layout_constraintEnd_toStartOf="@+id/fullNameGet"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/imageView3" />

    <TextView
        android:id="@+id/fullNameGet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintTop_toBottomOf="@id/imageView3"
        app:layout_constraintBottom_toTopOf="@+id/genderGet"
        app:layout_constraintStart_toEndOf="@+id/fullNameLabel"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:id="@+id/genderLabel"
        android:layout_width="115dp"
        android:layout_height="wrap_content"
        android:text="Gender"
        app:layout_constraintBottom_toTopOf="@+id/ageLabel"
        app:layout_constraintEnd_toStartOf="@+id/genderGet"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/fullNameLabel"/>

    <TextView
        android:id="@+id/genderGet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/ageGet"
        app:layout_constraintTop_toBottomOf="@+id/fullNameGet"
        app:layout_constraintStart_toEndOf="@+id/genderLabel"
        app:layout_constraintEnd_toEndOf="parent"/>


    <TextView
        android:id="@+id/ageLabel"
        android:layout_width="115dp"
        android:layout_height="wrap_content"
        android:text="Age"
        app:layout_constraintBottom_toTopOf="@+id/bloodGroupLabel"
        app:layout_constraintTop_toBottomOf="@id/genderLabel"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/ageGet"/>

    <TextView
        android:id="@+id/ageGet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/bloodGroupGet"
        app:layout_constraintTop_toBottomOf="@+id/genderGet"
        app:layout_constraintStart_toEndOf="@+id/ageLabel"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:id="@+id/bloodGroupLabel"
        android:layout_width="115dp"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/cityLabel"
        app:layout_constraintTop_toBottomOf="@id/ageLabel"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/bloodGroupGet"/>

    <TextView
        android:id="@+id/bloodGroupGet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBottom_toTopOf="@+id/cityGet"
        app:layout_constraintTop_toBottomOf="@+id/ageGet"
        app:layout_constraintStart_toEndOf="@+id/bloodGroupLabel"
        app:layout_constraintEnd_toEndOf="parent"/>


    <TextView
        android:id="@+id/cityLabel"
        android:layout_width="115dp"
        android:layout_height="wrap_content"
        android:text="City"
        app:layout_constraintBottom_toTopOf="@+id/phoneLabel"
        app:layout_constraintTop_toBottomOf="@id/bloodGroupLabel"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/cityGet"/>

    <TextView
        android:id="@+id/cityGet"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="TextView"
        app:layout_constraintBottom_toBottomOf="@+id/phoneGet"
        app:layout_constraintTop_toBottomOf="@+id/bloodGroupGet"
        app:layout_constraintStart_toEndOf="@+id/cityLabel"
        app:layout_constraintEnd_toEndOf="parent"/>

    <TextView
        android:layout_width="115dp"
        android:layout_height="wrap_content"
        android:id="@+id/phoneLabel"
        android:text="Mobile No."
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cityLabel"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/phoneGet"
        android:layout_marginBottom="40dp"/>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/phoneGet"
        android:text="Mobile No."
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toBottomOf="@id/cityGet"
        app:layout_constraintStart_toEndOf="@+id/phoneLabel"
        app:layout_constraintEnd_toEndOf="parent"
        android:layout_marginBottom="40dp"/>


</androidx.constraintlayout.widget.ConstraintLayout>